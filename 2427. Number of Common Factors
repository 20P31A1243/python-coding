class Solution:
    def commonFactors(self, a: int, b: int) -> int:
        c=0
        if(a>=b):
            s=b
        else:
            s=a
        for i in range(1,s+1):
            if(a%i==0 and b%i==0):
                c=c+1
        return c

Input: a = 12, b = 6
Output: 4
Explanation: The common factors of 12 and 6 are 1, 2, 3, 6
